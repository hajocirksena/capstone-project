  },
  "version": "0.1.0",
  "proxy": "http://localhost:8050",
  "private": true,
  "dependencies": {
    "@babel/core": "7.7.4",
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "7.7.6",
    "@babel/node": "^7.10.1",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.0.1",
    "express": "4.18.2",
    "mongoose": "5.8.1",
    "npm-run-all": "4.1.5"
  },
  "devDependencies": {
    "nodemon": "2.0.2"
  },
  "scripts": {
    "start": "env-cmd nodemon --exec babel-node server.js",
    "db:import": "export `cat .env`; mongoimport -d $DB_NAME -c items --jsonArray --file ./server/mockdata/cards.json",
    "db:delete": "export `cat .env`; mongo $DB_NAME --eval 'db.items.drop()' && echo 'Collection items dropped.'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
